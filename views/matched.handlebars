{{#each users as |user|}}
<div class="container">
    <div class="row">
        <img class="friend_pic" src="{{profile_pic}}" alt="{{first_name}}">
        <p>{{image}}</p>
        <p>{{first_name}}</p>
        <p>{{last_name}}</p>
        <p>{{bio}}</p>    
        <p>{{city}}</p>
        <p>{{state}}</p>
        <p>{{email}}</p>
        
    </div>
</div>
{{/each}}
